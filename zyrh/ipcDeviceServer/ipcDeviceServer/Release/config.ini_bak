[zyrhServer]
#httpPort http服务器与端口，内网使用
httpPort=8083
#httpDir http路径（未启用功能，但不能删除，配一个不存在的盘符）
httpDir=A://
#sdk登陆客户端接入网关（CAG)IP
sdkServerIp=127.0.0.1
#sdk登陆客户端接入网关(CAG)端口
sdkServerPort=23530
#sdk登陆客户端接入网关(CAG)线路号
sdkServerLine=0
#服务器停止流类型0:http停流接口无效,改为超时时间 1:http停流接口有效
ServerStopStreamType=0
#服务器停流时间单位分钟
StopStreamTimeOut=30

